{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/templates/(.*)",
            "dest": "$1"
        },
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
    ]
}
